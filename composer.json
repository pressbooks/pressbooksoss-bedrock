{
  "name": "pressbooks/pressbooksoss-bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": {
      "pressbooks/*": "source",
      "*": "dist"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "mnsami/composer-custom-directory-installer": true,
      "roots/wordpress-core-installer": true,
      "php-http/discovery": true
    },
    "platform": {
      "php": "8.2",
      "ext-redis": "3.1.1"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/lumenlearning/candela-citation"
    }
  ],
  "require": {
    "php": ">=8.2",
    "composer/installers": "2.3.0",
    "hypothesis/wp-hypothesis": "0.7.4",
    "pressbooks/pressbooks": "6.32.0",
    "pressbooks/pressbooks-aldine": "1.23.3",
    "pressbooks/pressbooks-book": "2.30.2",
    "pressbooks/pressbooks-cas-sso": "2.5.0",
    "pressbooks/pressbooks-clarke": "2.4.1",
    "pressbooks/pressbooks-donham": "2.1.1",
    "pressbooks/pressbooks-jacobs": "1.3.1",
    "pressbooks/pressbooks-network-catalog": "1.3.8",
    "pressbooks/pressbooks-saml-sso": "2.5.3",
    "roots/bedrock-autoloader": "1.0.4",
    "roots/bedrock-disallow-indexing": "2.0.0",
    "roots/multisite-url-fixer": "1.1.0",
    "roots/wordpress": "6.8.3",
    "roots/wp-config": "1.0.0",
    "vlucas/phpdotenv": "5.6.2",
    "wpackagist-plugin/blob-mimes": "1.4.2",
    "wpackagist-plugin/enable-media-replace": "4.1.7",
    "wpackagist-plugin/h5p": "1.16.0",
    "wpackagist-plugin/koko-analytics": "2.0.18",
    "wpackagist-plugin/parsedown-party": "1.2.1",
    "wpackagist-plugin/redirection": "5.5.2",
    "wpackagist-plugin/redis-cache": "2.7.0",
    "wpackagist-plugin/regenerate-thumbnails": "3.1.6",
    "wpackagist-plugin/tablepress": "3.2.3",
    "wpackagist-plugin/user-activation-keys": "4.6",
    "wpackagist-plugin/wp-quicklatex": "3.8.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "pressbooks/coding-standards": "^1.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
  }
}
